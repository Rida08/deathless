#include <iostream>

using namespace std;

const int N = 10e4;

int n, k, a[N];

bool check(int t) {
	int c = 1, l = 0;

	for (int i = 0; i < n; ++i) {
		if (a[i] > t) {
			return false;
		}
		if (l + a[i] <= t) {
			l += a[i];
		}else {
			++c;
			l = a[i];
		}
	}
	return c <= k;
}

int main(){
	cin >> n;

	for (int i = 0; i < n; i++) {
		cin >> a[i];
	}
	cin >> k;
			
	long l = 0, r = 33000;

	while (l + 1 < r){
		long m = (l + r) / 2;
		if (check(m)) {
			r = m;
		}else {
			l = m;
		}
	}
	cout << r;
}
