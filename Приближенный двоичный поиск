#include <iostream>
#include <string>
using ll = long long;
int main(){
	ll n, k;
	std::cin >> n >> k;
	ll* a = new ll[n];
	ll* b = new ll[k];
	for (int i = 0; i < n; i++)
		std::cin >> a[i];
	for (int i = 0; i < k; i++)
		std::cin >> b[i];
	for (int i = 0; i < k; i++)
	{
		int l = -1, r = n, m = n / 2;
		while (r - l > 1) {
			a[m] <= b[i] ? l = m : r = m;
			m = (l + r) / 2;
		}
		std::cout << a[r-1] << "\n";
	}
}
