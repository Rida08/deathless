#include <iostream>
#include <vector>



using namespace std;
using ll = long long;

int n, m, kol=1e9+1;
vector<int>a;

void dt(int i, int k, int s) {
	if (s == n && k < kol) {
		kol = k;
		return;
	}
	if (i == 2 * m) return;
	dt(i + 1, k + 1, s + a[i]);
	dt(i + 1, k, s);
}

int main() {
	cin >> n >> m;
	int x, s1=0;
	for (int i = 0; i < m; ++i) {
		cin >> x;
		a.push_back(x);
		a.push_back(x);
		s1 += x * 2;
	}
	if (s1 < n) {
		cout << "-1";
		return 0;
	}else if (s1 == n) {
		cout << a.size();
		return 0;
	}

	dt(0, 0, 0);

	if (kol == 1e9 + 1) {
		cout << "0";
		return 0;
	}
	cout << kol;
}
