#include <iostream>
#include <string>
#include <algorithm>
#include <vector>

using ll = long long;
using namespace std;

const int N = 1e4 * 4;
bool used[N];
vector<int>primes;

ll a, b;

string an = "";

void solve() {
	cin >> a >> b;
	ll x = abs(a - b);
	int f = 1;
	for (int p : primes) {
		if (x < p * p) {
			break;
		}
		while (x % p == 0) {
			f += 1;
			x /= p;
		}
	}
	if (x > 1) {
		f += 1;
	}
	an += to_string(f) + "\n";
}

int main() {
	int t;
	cin >> t;
	for (int i = 2; i < N; ++i) {
		if (used[i]) {
			continue;
		}
		for (ll j = i * i; j < N; j += i) {
			used[j] = 1;
		}
	}
	for (int i = 2; i < N; ++i) {
		if (!used[i]) {
			primes.push_back(i);
		}
	}
	for(int i=0; i<t; ++i){
		solve();
	}
	cout << an;
}
