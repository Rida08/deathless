#include <iostream>
#include <vector>



using namespace std;
using ll = long long;

int n, W, res=1e5;
vector<int>w, a;

void dt(int n1, int s, int p){
    for(int i=n1; i<n; ++i){
        if(s+w[i]>W){
            if(p<res){
                res=p;
            }
            return;
        }
          dt(i, s+w[i], p+a[i]);
    }
}

int main(){
    cin>>n>>W;
    for(int i=0; i<n; ++i){
        int x, y;
        cin>>x>>y;
        w.push_back(x), a.push_back(y);
    }
    dt(0, 0, 0);
    cout<<res;
}
