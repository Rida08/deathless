	#include <iostream>
	#include <string>
	#include <algorithm>
	#include <vector>
	using namespace std;
	int main()
	{
		long t, n, q, l, x;
		cin >> t;
		string an = "";
		for (int i = 0; i < t; ++i) {
			cin >> n >> q;
			vector<int>a(n, 0);
			vector<int>s(n+1, 0);
			for (int j = 0; j < n; ++j) cin >> a[j];
			sort(begin(a), end(a));
			s[0] = a[0];
			for(int j=1; j<n; ++j)	s[j] += s[j - 1] + a[j];
			for (int j = 0; j < q; ++j) {
				cin >> x;
				if (s[n - 1] < x) an += "-1\n";
				else {
					long r = n, l = 0, m = r / 2;
					while (r - l != 1) {
						s[m] >= x ? r = m : l = m;
						m = (l + r) / 2;
					}
					an += to_string(r) + "\n";
				}
			}
		}
		cout << an;
	}
